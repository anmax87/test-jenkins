node('AgentDocker')
stage('Checkout external proj') {
    git (branch: 'master',
    credentialsId: 'git',
    url: 'git@github.com:anmax87/test-jenkins.git')

    sh "ls -lat"
}
stage('Check terraform version') {
    sh "terraform -v"
}
stage('Add cred & terraform plan') {
        withCredentials([string(credentialsId: 'access_key', variable: ''), string(credentialsId: 'secret_key', variable: '')]) {
            sh "export AWS_DEFAULT_REGION='us-west-2'"
            sh "terraform init"
            sh "terraform plan"
        }
}
